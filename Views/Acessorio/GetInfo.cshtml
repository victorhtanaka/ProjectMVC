@model Acessorio;
@{
    ViewData["Title"] = "Ver Acessorio";
}
<h1>Informacoes Detalhadas</h1>
    <div class="row">

        <div class="col-lg-12">

            <div class="form-group">
                <label>Codigo:</label>
                <p>@Model.CodAcessorio</p>
            </div>

            <div class="form-group">
                <label>Nome:</label>
                <p>@Model.NomeAcessorio</p>
            </div>

        </div>

    </div>