@model Relatorio;
@{
    ViewData["Title"] = "Ver Relatorio";
    Agenda? agenda = ViewData["Agenda"] as Agenda;
    List<Funcionario>? Funcionarios = ViewData["Funcionarios"] as List<Funcionario>;
    List<Cliente>? Clientes = ViewData["Clientes"] as List<Cliente>;
    List<Servico>? Servicos = ViewData["Servicos"] as List<Servico>;
    var cliente = Clientes.FirstOrDefault(c => c.CodCliente == agenda.FkClienteCodCliente);
    var servico = Servicos.FirstOrDefault(s => s.CodServico== agenda.FkServicoCodServico);
    var funcionario = Funcionarios.FirstOrDefault(f => f.CodFuncionario == agenda.FkFuncionarioCodFuncionario);
}
<h1>Informacoes Detalhadas</h1>
    <div class="row">

        <div class="col-lg-12">

            <div class="form-group">
                <label>Codigo do relatorio:</label>
                <p>@Model.CodRelatorio</p>
            </div>

            <div class="form-group">
                <label>Descricao:</label>
                <p>@Model.DescRelatorio</p>
            </div>

            <div class="form-group">
                <label>Codigo do agendamento:</label>
                <p>@Model.FkAgendaCodAgenda</p>
            </div>

            <div class="form-group">
                <label>Cliente:</label>
                <p>@cliente.NomeCliente</p>
            </div>

            <div class="form-group">
                <label>CPF do cliente:</label>
                <p>@cliente.NomeCliente</p>
            </div>

            <div class="form-group">
                <label>Servico solicitado:</label>
                <p>@servico.CodServico</p>
            </div>

            <div class="form-group">
                <label>Funcionario encarregado:</label>
                <p>@funcionario.NomeFuncionario</p>
            </div>

        </div>

    </div>